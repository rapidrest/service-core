{
    "name": "@rapidrest/service-core",
    "version": "1.0.0",
    "description": "Provides all core functionality for RapidREST based backend services.",
    "repository": "https://github.com/rapidrest/service-core.git",
    "author": "RapidREST <rapidrests@gmail.com>",
    "license": "MIT",
    "contributors": [
        "Jean-Philippe Steinmetz",
        "Arthur Thompson",
        "Thomas Buscaglia"
    ],
    "scripts": {
        "build": "rimraf dist && tsc",
        "deploy-docs": "ts-node tools/gh-pages-publish",
        "docs": "typedoc --plugin typedoc-plugin-markdown --out docs/reference src",
        "lint": "eslint ./src ./test",
        "local-audit": "yarn install && yarn npm audit -A -R \"$@\"",
        "checkup": "yarn dlx @yarnpkg/doctor \"$@\"",
        "dependency-check": "yarn dlx depcheck \"$@\"",
        "report-coverage": "cat ./coverage/lcov.info | coveralls",
        "test": "node --loader ts-node/esm ./node_modules/vitest/vitest.mjs run",
        "test:prod": "npm run lint && node --loader ts-node/esm ./node_modules/vitest/vitest.mjs run",
        "postversion": "git push && git push --tags"
    },
    "dependencies": {
        "axios": "^1.13.4",
        "dayjs": "^1.11.19",
        "deepmerge": "^4.3.1",
        "node-schedule": "^2.1.1",
        "response-time": "^2.3.4",
        "swagger-ui-express": "^5.0.1",
        "winston-transport": "^4.9.0"
    },
    "peerDependencies": {
        "@rapidrest/core": "1.x",
        "cors": "2.x",
        "express": "5.x",
        "express-session": "1.x",
        "ioredis": "5.x",
        "lodash": "4.x",
        "mongodb": "6.x",
        "openapi3-ts": "4.x",
        "passport": "0.7.x",
        "passport-strategy": "1.x",
        "prom-client": "15.x",
        "reflect-metadata": "0.2.x",
        "winston": "3.x",
        "ws": "8.x"
    },
    "devDependencies": {
        "@rapidrest/core": "^1.0.2",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/express-session": "^1.18.2",
        "@types/ioredis-mock": "^8.2.6",
        "@types/jest": "^30.0.0",
        "@types/node": "^25.2.0",
        "@types/node-schedule": "^2.1.8",
        "@types/passport": "^1.0.17",
        "@types/passport-strategy": "^0.2.38",
        "@types/response-time": "^2.3.9",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.6",
        "coveralls": "^3.1.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsdoc": "^62.5.0",
        "express": "^5.2.1",
        "express-session": "^1.19.0",
        "globals": "^17.3.0",
        "ioredis": "^5.9.2",
        "ioredis-mock": "^8.13.1",
        "jest": "^30.2.0",
        "jest-junit": "^16.0.0",
        "lodash": "^4.17.23",
        "mongodb": "^6.12.0",
        "mongodb-memory-server": "^11.0.1",
        "nconf": "^0.13.0",
        "openapi3-ts": "^4.5.0",
        "passport": "^0.7.0",
