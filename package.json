        "vitest": "^4.0.18",
        "winston": "^3.19.0",
        "ws": "^8.19.0"
    },
    "resolutions": {
        "cross-spawn": "^7.0.5"
    },
    "packageManager": "yarn@4.5.3",
    "engines": {
        "node": ">=20.0.0"
    },
